VERSION 5.5 ; 
NAMESCASESENSITIVE ON ;
DIVIDERCHAR "/" ;
BUSBITCHARS "[]" ;

DESIGN reg1 ;
TECHNOLOGY technology ;

UNITS DISTANCE MICRONS 1000 ;

DIEAREA ( -1000 -1000 ) ( 1000 1000 ) ;


COMPONENTS 9 ;
- buffer1 snl_bufx2
+ PLACED ( 100000 400000 ) N ;
- buffer2 snl_bufx2
+ PLACED ( 400000 100000 ) N ;
- buffer3 snl_bufx2
+ PLACED ( 200000 100000 ) N ;
- buffer4 snl_bufx2
+ PLACED ( 100000 200000 ) N ;
- r1 snl_ffqx1
+ PLACED ( 100000 200000 ) N ;
- r2 snl_ffqx1
+ PLACED ( 200000 100000 ) N ;
- r3 snl_ffqx1
+ PLACED ( 300000 300000 ) N ;
- u1 snl_bufx1
+ PLACED ( 400000 100000 ) N ;
- u2 snl_and02x1
+ PLACED ( 100000 400000 ) N ;
END COMPONENTS

PINS 6 ;
- in1 + NET in1 + DIRECTION INPUT + USE SIGNAL 
  + LAYER M4 ( -100 0 ) ( 100 1040 ) + FIXED ( 100000 200000 ) N ;
- clk + NET clk + DIRECTION INPUT + USE SIGNAL 
  + LAYER M4 ( -100 0 ) ( 100 1040 ) + FIXED ( 100000 100000 ) N ;
- out + NET out + DIRECTION OUTPUT ;
END PINS

SPECIALNETS 2 ;
- VSS  ( * VSS )
  + USE GROUND ;
- VDD  ( * VDD )
  + USE POWER ;
END SPECIALNETS

NETS 11 ;
- clk ( PIN clk ) ( r3 CP ) ( r2 CP ) ( r1 CP ) ;
- in1 ( PIN in1 ) ( r2 D ) ( r1 D ) ;
- net1 ( r3 D ) ( buffer1 Z ) ;
- net2 ( u2 B ) ( buffer2 Z ) ;
- net3 ( u1 A ) ( buffer3 Z ) ;
- net4 ( u2 A ) ( buffer4 Z ) ;
- out ( PIN out ) ( r3 Q ) ;
- r1q ( buffer4 A ) ( r1 Q ) ;
- r2q ( buffer3 A ) ( r2 Q ) ;
- u1z ( buffer2 A ) ( u1 Z ) ;
- u2z ( buffer1 A ) ( u2 Z ) ;
END NETS

END DESIGN
